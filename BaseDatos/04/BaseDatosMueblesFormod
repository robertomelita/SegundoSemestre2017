EMPRESA(*Rut, nombre, telefono, direccion,#RutEmpresa)
CLIENTE(*Rut, nombre, apellido, telefono)
COTIZACION(*ID, #RutCliente)
TIPOPRODUCTO(*ID, descripcion, precio)
CONTIENE(*(#IDCotizacion, #IDProducto), cantidad)
MATERIAL(*ID, tipo)
REQUIERE(*(#IDProducto,#IDMaterial),cantidad)
CARACTERISTICA(*Nombre)
POSEE(*(#Nombre, #IDMaterial), valor)
ESTADO(*(#IDCotizacion, fecha), valorEstado)
STOCK(*(#IDMaterial, fecha), stock)
